/* 点赞和评论列表 */
.discuss-container { display: -webkit-flex; display: flex; flex-direction:row; padding-top: 32rpx;}

.discuss-block{
  flex:1;
  height: auto;
  position: relative;
  background-color: #f6f6f6;
}

/*  点赞列表start  */
.like-list{
  display: -webkit-flex;
  display: flex;
  justify-content: flex-start;
  padding: 10rpx 20rpx 20rpx 20rpx;
}
.like-item { display: -webkit-flex; display: flex; align-items: center; }
.like-icon { margin: 10rpx 20rpx 0 0; }
.like-icon image { width: 24rpx; height: 22rpx;position: absolute;left: 15rpx;top:28rpx; }
.like-list-view { width:400rpx; overflow:hidden; flex-wrap: wrap; height: 50rpx;padding-left: 15rpx }
.like-image-view image { width:40rpx; height:40rpx; margin-right: 10rpx; margin-top:10rpx; border-radius: 8rpx; }
.like-count { color: #999999; font-size:24rpx; margin-left: 18rpx; margin-top:12rpx; }
.like-bg { position: absolute; width:25rpx; height: 11rpx; top: -10rpx; right: 6rpx; }
/* 点赞列表end */

/* 详情页点赞列表start */
.like-detail-view { width:100%; flex-wrap: wrap;padding-left: 15rpx }
/* 详情页点赞列表end */

.line-view { width:100%; height:1rpx; background-color:#e2e2e2; } /* 分隔线 */

/* 评论列表start */
.discuss-list{
    width:100%;
    font-size:26rpx; color:#333333;
    display: -webkit-flex;
    display: flex;
    flex-direction:column;
    justify-content: flex-start;
    padding: 10rpx 0;
}
.discuss-name { color: #667494; font-weight: 500; white-space:nowrap;display: inline-block}
.comment-list-l{
  display: inline-block;
  white-space:nowrap ;
  word-break:break-all
}
.discuss-item{ padding: 6rpx 20rpx 6rpx 20rpx; width:550rpx; line-height: 35rpx; }
.discuss-list-item { overflow: hidden; text-overflow:ellipsis; white-space: nowrap; }
.discuss-detail-item { width: 650rpx; word-wrap: break-word; word-break:break-all}
.discuss-item-result { color:#7a97da; font-size:22rpx; }
/* 评论列表end */

