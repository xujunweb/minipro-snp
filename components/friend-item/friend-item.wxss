/* components/friend-item/friend-item.wxss */
.goods_row{
  border-bottom: 1rpx solid #e6e6e6;
  padding-bottom:40rpx;
  margin-bottom: 30rpx;
  position: relative;
  background: #fff;
}
.goods_row .goods_left{
  margin-left: 28rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 8rpx;
  background-color: #eee;
  position: absolute;
  top:0; left: 0;
}
.goods_row .goods_left .head_img{
  width: 80rpx;
  height: 80rpx;
  border-radius: 8rpx;
}
.goods_row .goods_left .symbol_img {
  position: absolute;
  width:29rpx;
  height: 29rpx;
  bottom: -12rpx;
  right:-2rpx;
}
.goods_row .goods_right{
  width:100%;
  box-sizing: border-box;
  padding-left: 132rpx;
  padding-right: 30rpx;
}
.goods_row .goods_right .goods_right_a{
  margin-top:-4rpx;
  display: flex;
  display: -webkit-flex;
}
.goods_row .goods_right .goods_right_a view {
  display:line-block;
  height:32rpx;
  line-height:30rpx;
  box-sizing: border-box;
}
.goods_row .goods_right .goods_right_a .goods_orgname {
  font-size: 32rpx;
  color: #667494;
  font-weight: 500;
  max-width: 360rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.goods_row .goods_right .goods_right_a .goods_orgname-long{
  font-size: 32rpx;
  color: #667494;
  font-weight: 500;
  max-width: 480rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.goods_row .goods_right .goods_right_a .goods_orgname-long-long{
  font-size: 32rpx;
  color: #667494;
  font-weight: 500;
  max-width: 550rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.goods_row .goods_right .goods_right_a .goods_set_top{
  width: 32rpx; height: 32rpx; line-height: 32rpx;
  text-align: center;
  font-size:20rpx;
  background-color: #d8e1f4;
  color: #667494;
  border-radius: 4rpx;
  margin-left: 12rpx;
}
.goods_row .goods_right .goods_right_b{
  font-size:30rpx;
  color: #333;
  line-height: 42rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-wrap: break-word;
  margin-top: 12rpx;
}

.goods_row .goods_right .goods_right_b.common{
  -webkit-line-clamp: 5;
}

.goods_row .goods_right .goods_right_b text{
  word-break: break-all;
}

.category-tag-name{
  color: #a4a6aa;
  padding-right: 13rpx;
}

.goods_row .goods_right .goods_right_c{
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 20rpx;
  /* 最多显示9张图片，超出隐藏 */
  max-height: 494rpx;
  overflow: hidden;
}
.goods_row .goods_right .goods_right_c .goods_images{
  width: 158rpx;
  height: 158rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  background-color:#eee;
}
.goods_row .goods_right .goods_right_c .goods_image{
  width: 360rpx;
  height: 360rpx;
  margin-bottom:8rpx;
  background-color:#eee;
}
.goods_row .goods_right .goods_price{
  max-width: 266rpx;
  height: 54rpx;
  border-radius: 8rpx;
  float: right;
  text-align: center;
  margin-right: 90rpx;
  border: 2rpx solid #ff7800;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}
.goods_row .goods_right .goods_price text{
  color: #ff7800;
  font-size: 24rpx;
  line-height: 54rpx;
}
.goods_row .goods_right .goods_address{
  color:#667494;
  font-size:24rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  clear: both;
  padding-top: 10rpx;
  line-height: 24rpx;
  margin-right:50rpx;
}
.goods_row .goods_right .goods_time{
  font-size:24rpx;
  color: #999;
  line-height: 24rpx;
  margin-top: 20rpx;
  margin-bottom: 5rpx;
  clear: both;
}
.goods_row .goods_right .goods_time .goods_read{
  background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/pingtaixcx/view1213.png) left center / 30rpx 20rpx no-repeat;
  padding-left: 42rpx;
  margin-left: 50rpx;
  display: inline-block;
  height: 22rpx;
}
.goods_row .goods_right .goods_time .goods_del{
   font-size:24rpx;
   line-height: 24rpx;
   margin-top: 30rpx;
   margin-left: 28rpx;
   color: #667494;
   display: line-block;
   padding: 0 20rpx;
}
.goods_row .goods_right .goods_right_d{
  position: relative;
}

.goods_row.first{
    padding-top: 30rpx;
}
.goods_row.first .goods_left{
    top: 30rpx;
}
.goods_row .goods_right .goods_right_a {
    margin-top: 0;
}
.goods_row.last{
  border-bottom: none;
  margin-bottom: 0;
}

.article_link{margin: 20rpx 0 24rpx 0;  width: 100%; height: 100rpx; display: flex; padding: 10rpx; background-color: #F6F6F6; box-sizing: border-box}
