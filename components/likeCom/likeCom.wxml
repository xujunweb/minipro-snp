<!--components/likeCom/likeCom.wxml-->
<view class="goods_row_5" style="border-top:{{border?'1rpx solid #e2e2e2':'none'}};height:{{height?height+'rpx':'50rpx'}}">
  <view class='goods_row_pl'><text>{{item.comment ? item.comment : 0}}</text></view>
  <view class='{{item.likeStatus == 1 ? "goods_row_dz_2" : "goods_row_dz_1"}}' data-index="{{i}}" data-moment-id="{{item.id}}" data-like-status="{{item.likeStatus}}" data-user-type="{{item.userType}}" data-like-id="{{item.likeId}}" catchtap="onPraise">
      <text>{{item.like > 0 ? item.like : "èµž"}}</text>
  </view>
  <view class='goods_row_5_ll'><text>{{item.comment}}</text></view>
</view>
