/*分类*/
.classification{
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  position: relative;
  background-color: white;
  display: flex; padding-left: 0; justify-content:space-around;
  border-bottom: 1rpx solid #e2e2e2;
}
.classification.fixed {
  position: fixed; left: 0; top: 0;
  z-index: 666; background-color: white;
}
.classification .scroll-view-item_H{
  width: auto; text-align: center;
  position: static; line-height: 90rpx; height: 89rpx;
}
.classification .scroll-view-item_H text { 
  display: inline-block; 
  font-size:28rpx;
  color: #222;
}
.scroll-view-item_H text.cur{ color:#ff7800; font-weight: 600;  }
.card_rank button { 
  display: inline-block; width: auto; color: #666; font-size: 28rpx; border:none;
}
.classification2 .scroll-view-item_H text {
  /* margin: 0 42rpx; */
}


