.recommend_follow{
  padding: 40rpx;
  padding-top: 36rpx;
}
.recommend_follow .follow_title{
  margin-bottom:37rpx;
  height:14px;
  display:flex;
  align-items:center;
  flex-direction:row;
}
.recommend_follow .follow_title image{
  width:27rpx;
  height:27rpx;
  margin-right:7rpx;
}
.recommend_follow .follow_title text{
  font-size:28rpx;
  background:linear-gradient(#ffa500, #ff7800);
  -webkit-background-clip:text;
  color:transparent;
  line-height:28rpx;
}
.recommend_follow .follow_list .list_item{
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: row;
}
.recommend_follow .follow_list .list_item .item_l{
  width: 104rpx;
  height: 104rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.recommend_follow .follow_list .list_item .item_l image{
  width: 104rpx;
  height: 104rpx;
  border-radius: 50%;
}
.recommend_follow .follow_list .list_item .item_r{
  flex: 1;
  overflow: hidden;
  border-bottom: 1rpx solid #e2e2e2;
  padding-bottom: 36rpx;
}
.recommend_follow .follow_list .list_item .item_r .warp{
  float: left;
}
.recommend_follow .follow_list .list_item .item_r .warp .card_name{
  font-size: 36rpx;
  color: #000000;
}
.recommend_follow .follow_list .list_item .item_r .warp .card_position{
  font-size: 28rpx;
  color: #999;
}
.recommend_follow .follow_list .list_item .item_r .follow{
  float: right;
  width: 116rpx;
  height: 56rpx; 
  line-height: 56rpx;
  text-align: center;
  border: 2rpx solid #ff7800;
  color: #ff7800;
  font-size: 24rpx;
  border-radius: 30rpx;
  margin-top: 22rpx;
}
.recommend_follow .follow_list .list_item .item_r .stop_follow{
  color: #ccc;
  border: 2rpx solid #ccc;
}

/* 系统消息 */
.sys_msg{
  position: fixed; top: 20rpx; left: 10rpx; width: 730rpx; height: 80rpx;
  border-radius: 10rpx; background-color: rgba(255,255,255,0.9);
  display: flex; align-items: center; z-index: 999;
}
.sys_icon { width: 40rpx; height: 40rpx; margin-left: 30rpx; }
.sys_icon image { width: 40rpx; height: 40rpx; border-radius: 40rpx; }
.sys_text { 
  font-size: 26rpx; color: #333; margin-left: 12rpx; width: 610rpx;
  overflow: hidden; 
  text-overflow: ellipsis; 
  display: -webkit-box; 
  -webkit-line-clamp: 1; 
  -webkit-box-orient: vertical;
  word-break:break-all;
}
.sys_next{
  width: 12rpx; height: 26rpx;
  background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/xcx/msg_next.png) no-repeat center center/12rpx 26rpx;
}


