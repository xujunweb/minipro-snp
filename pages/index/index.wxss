}
.search-text {
  background-image: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/xcx/search1.png);
  background-repeat: no-repeat; background-size: 26rpx 26rpx; background-position: 0 6rpx;
  position: absolute; top: 32rpx; left: 60rpx; font-size: 26rpx; color: rgba(102,102,102,0.9);
  padding-left: 40rpx;
}
.share_code_bg .share_code{
  background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/xcx/code_icon1.png) no-repeat;
  position: absolute;
  background-size: 54rpx 50rpx;
  top: 28rpx;
  right: -15rpx;
  width: 100rpx;
  height: 54rpx;
}
.platform_info_item { flex: 1 }
