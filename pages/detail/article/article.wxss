/* pages/detail/article/article.wxss */
page {
  height: 100%;
  position: relative;
}

.goods {
  padding: 20rpx 0 34rpx;
  background-color: #fff;
}

.goods_content {
}

.goods_row {
  margin: 30rpx;
  position: relative;
  margin-bottom: 0;
}

.goods_row .goods_left {
  width: 70rpx;
  height: 70rpx;
  background-color: #fff;
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -41rpx;
}

.goods_left .head_img {
  width: 70rpx;
  height: 70rpx;
  border-radius: 35rpx;
  display: block;
  overflow: hidden;
  background-color: #eee;
}

.goods_row .goods_left view {
  position: absolute;
  top: 0;
  left: 0;
  width: 70rpx;
  height: 70rpx;
  z-index: 3333;
  opacity: 0;
}

.symbol_img {
  position: absolute;
  width: 29rpx;
  height: 29rpx;
  bottom: -2rpx;
  right: -2rpx;
}

.goods_row .goods_right {
  width: 100%;
  position: relative;
  box-sizing: border-box;
  padding-left: 92rpx
}

.contact_2 {
  position: absolute;
  padding: 0 30rpx;
  height: 50rpx;
  right: 0;
  top: 9.5rpx;
  background: #9da3f1;
  color: #fff;
  font-size: 24rpx;
  line-height: 50rpx;text-align: center;
  border-radius: 50rpx;
}

.contact_2 image {
  width: 45rpx;
  height: 39rpx;
  margin: 10rpx;
}

/* 已创建名片图标 */
.card_label {
  width: 30rpx;
  height: 20rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}

.goods_right_a {
  position: relative;
}
.goods_right_a .type{
  font-size: 24rpx;color: #999;
}
.goods_right_a view {
  /* display: line-block;
  line-height: 38rpx;
  box-sizing: border-box; */
}

.goods_orgname {
  font-size: 32rpx;
  color: #667494;
  font-weight: 500;
  max-width: 560rpx;
  max-height: 76rpx;
  overflow: hidden;
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 10rpx;
}

.goods_set_top {
  width: 88rpx;
  text-align: center;
  font-size: 20rpx;
  background-color: #d8e1f4;
  color: #667494;
  border-radius: 8rpx;
  margin-left: 10rpx;
}
/* 免费入驻 */
.free_join_view {
  position: absolute;
  top: 0;
  right: 0rpx;
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 8rpx;
  text-align: center;
  border: 2rpx solid #ff7800;
  color: #ff7800;
  background-color: #fff;
  font-size: 24rpx;
}

.goods_right_b {
  font-size: 26rpx;
  color: #333;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  max-width: 690rpx;
  display: block;
}

.goods_right_b text {
  max-width: 690rpx;
  display: block;
  word-break: break-all;
}

.goods_right_c {
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 20rpx;
}

.goods_right_c .goods_image {
  width: 690rpx;
  margin-bottom: 10rpx;
  background-color: #eee;
  height: auto;
}

.goods_right_c view:last-of-type .goods_image {
  margin-bottom: 0;
}

.goods_right_c view:last-of-type {
  line-height: 1px;
}

/* 浏览数,地址，时间，删除 */
.goods_address {
  color: #667494;
  font-size: 24rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  line-height: 24rpx;
  word-break: break-all;
  margin: 30rpx 0;
  width: 100%;
}

/* 商品被删除页面样式 */
.del_goods_page {
  width: 100%;
  height: 100%;
  background-color: #f4f4f4;
}

.warn_icon {
  padding-top: 360rpx;
  text-align: center;
}

.del_desc {
  font-size: 30rpx;
  color: #666666;
  text-align: center;
  padding-top: 30rpx;
}

/*立即购买*/
.buy {
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  border-top: 1rpx solid #e6e6e6;
  width: 100%;
  background-color: #fff;
  z-index: 999;
}

.buy .buy_left {
  background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/one-shop-one-mini-app/detail_shoppingcar.png) 30rpx center / 53rpx 49rpx no-repeat;
  padding-left: 109rpx;
  color: #666;
  flex: 1;
}

.buy .buy_left .buy_price {
  font-size: 36rpx;
  font-family: 'WeChatNumber-151125';
  margin-top: 28rpx;
}

.buy .buy_right {
  width: 300rpx;
  background-color: #ff7800;
  text-align: center;
  color: #fff;
  line-height: 100rpx;
  font-size: 36rpx;
}

.buy .buy_right_disable {
  background-color: #ddd;
}

/* 顶部商品价格 */
.goods_activity_price {
  overflow: hidden;
  padding-left: 26rpx;
  padding-right: 31rpx;
}

.goods_price {
  float: left;
  color: #ff7800;
  font-weight: 500;
  font-size: 36rpx;
  line-height: 36rpx;
  margin-right: 21rpx;
}

.goods_label {
  float: left;
  width: 80rpx;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  font-size: 22rpx;
  border: 2rpx solid #ff7800;
  color: #ff7800;
  border-radius: 4rpx;
}

.goods_stock {
  float: right;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #888;
}

.goods_cost_price {
  padding: 0 31rpx;
}

.goods_cost_price_1 {
  text-decoration: line-through;
  color: #aaa;
  font-size: 28rpx;
  line-height: 28rpx;
  margin-top: 20rpx;
}

.goods_cost_price_2 {
  border-bottom: 1rpx solid #e2e2e2;
  padding-bottom: 40rpx;
}

.goods_title {
  font-size: 34rpx;
  color: #222;
  padding: 0 0rpx 22rpx 0rpx;
  line-height: 56rpx;
  font-weight: 500;
  word-break: break-all;
}
.share_btn {
  background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/pingtaixcx/share.png) left center / 44rpx no-repeat;
  width: 44rpx;
  height: 44rpx;
  margin: 16rpx 20rpx 0 20rpx;
}

.container {
  min-height: inherit!important;
  background-color: #f4f4f4;
}



/* 关注按钮 */
.attention1 {
  width: 120rpx;
  line-height: 38rpx;
  color: #ff7800;
  border: 2rpx solid #ff7800;
  border-radius: 38rpx;
  text-align: center;
  font-size: 22rpx;
  position: absolute;
  top: 50%;
  right: 0;
  margin-top: -19rpx;
}

.attention2 {
  color: #ccc;
  border: 2rpx solid #ccc;
}

/* 时间和联系ta */
.goods_time {
  position: relative;
  height: 24rpx;
  line-height: 24rpx;
}

.goods_time .time {
  margin-right: 50rpx;
}

.goods_time .time,.goods_time .read-num {
  font-size: 24rpx;
  color: #999;
  line-height: 24rpx;
  display: inline-block;
}

.goods_time .del {
  margin-left: 50rpx;
  color: #667494;
  font-size: 24rpx;
}

.goods_time .tel_he {
  position: absolute;
  right: 0;
  top: -4rpx;
  width: 86rpx;
  line-height: 32rpx;
  border: 2rpx solid #667494;
  color: #667494;
  font-size: 20rpx;
  text-align: center;
  border-radius: 8rpx;
}

.goods_charge {
  color: #999;
  font-size: 24rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  margin-top: 4rpx;
  width: 468rpx;
  word-break: break-all;
}
.discuss-top{
  margin-top:20rpx;
}

.replace-video{ position: relative; height: 410rpx; }
.mask-img{ background: rgba(0,0,0,0.5); position: absolute; top: 0; left: 0;  width:690rpx; height: 410rpx;}
.play-icon{ position: absolute; z-index: 2; top: 50%; left: 50%; width: 93rpx; height: 93rpx; margin-left: -60rpx; margin-top: -46rpx; background: url(http://yamxcx.oss-cn-shenzhen.aliyuncs.com/xcx/player.png) no-repeat center center; background-size: 100% 100%; }
.no_price_hide{ display: none !important; }

@keyframes redAnimation {
    25% { transform: rotate(14deg); }
    50% { transform: rotate(0deg); }
    75% { transform: rotate(-14deg); }
    100% { transform: rotate(0deg); }
}

  /* 海报弹框 */
  .maskBox{
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.7);
    z-index: 5555;
  }
  .poster{ 
    position: fixed;
    top:10%;
    left:50%;
    margin-left:-325rpx;
    width:650rpx;
    height: auto;
    background: #fff;
    border-radius: 30rpx;
  }
  .posterImg{ 
    box-shadow:0 0 20rpx 0 rgba(154, 154, 154, 0.23);
    width:570rpx;
    margin:40rpx;
    border-radius: 20rpx;
    display:block;
  }
  .posterText{
    color:#999;
    font-size:26rpx;
    line-height: 28rpx;
    text-align: center;
    padding-bottom: 40rpx;
  }
  .posterBtn{
    width:100%;
    font-size:34rpx;
    color:#ff7800;
    height:100rpx;
    line-height: 100rpx;
    text-align: center;
    border-top: 1px solid #e2e2e2;
  }

