/* pages/my/feedback/feedback.wxss */
.conception{width:100%; margin:0 auto; background-color:#fff;overflow: hidden;margin-top: 20rpx;}
.conception .conception_text textarea{width:690rpx; margin:0 auto; margin-top:25rpx; color:#000; height:400rpx; font-size:30rpx;}


.bottom-submit-part{
	position: fixed;
	display: flex;
	flex-direction: row;
	justify-content: center;
	bottom: 0;
	left: 0;
	margin-top: 0rpx;
	margin-bottom: 0rpx;
	width: 100%;
	height: 100rpx;
	z-index: 1000;
}
.bottom-submit-part .right-data-part{
	display: flex;
	justify-content: center;
	flex: 1;
	height: 100rpx;
	align-items: center;
	border-style: none;
	border-radius: 0rpx;
	font-size: 34rpx;
	color: #ffffff;
}
.submit-able-class{
	background-color: #9da3f1;
}
.submit-disable-class{
	background-color: #d6d6d6;
}